<%- contentFor('container') %>

<h1>LOGIN</h1>
<h1>30 Habit Steps</h1>
<form action="/login" method="post" class="loginForm">
  <div class="form-group">
    <label for="exampleInputEmail1">Email address</label>
    <input name="username" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input name="password" type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button> 
  <button class="btn btn-primary"><a href="/register">회원가입</a></button>
</form>




<%- contentFor('script') %>

